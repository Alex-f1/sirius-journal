@font-face {
  font-family: "ONYForm";
  src: 
    url("../fonts/ONYForm-Medium.otf") format("opentype");
    url("../fonts/ONYForm-Medium.woff2") format("woff2");
  font-weight: 500;
  font-style: normal;
  font-display: swap;
}
@font-face {
  font-family: "ONYSans";
  src: 
    url("../fonts/ONYSans-Book.otf") format("opentype");
    url('../fonts/ONYSans-Normal.woff2') format('woff2')
  font-weight: 400;
  font-style: normal;
  font-display: swap;
}
@font-face {
  font-family: "ONYSerif";
  src: 
    url("../fonts/ONY-Serif-Regular.otf") format("opentype");
    url('../fonts/ONY-Serif-Regular.woff2') format('woff2')
  font-weight: 400;
  font-style: normal;
  font-display: swap;
}

@font-face {
  font-family: 'Source Serif Pro';
  src: url('../fonts/SourceSerifPro-SemiBold.woff2') format('woff2'),
      url('../fonts/SourceSerifPro-SemiBold.woff') format('woff');
  font-weight: 500;
  font-style: normal;
  font-display: swap;
  unicode-range: U+0040
}

@font-face {
  font-family: 'Source Serif Pro';
  src: url('../fonts/SourceSerifPro-Regular.woff2') format('woff2'),
      url('../fonts/SourceSerifPro-Regular.woff') format('woff');
  font-weight: 400;
  font-style: normal;
  font-display: swap;
  unicode-range: U+0040
}

